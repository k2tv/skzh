    181            {
    182              // Put code here to handle Power-On reset
    183            }
    184            else if ( rib == RESETEX )
    185            {
    186              // Put code here to handle External reset
    187            }
    188            else if ( rib == RESETWD )
    189            {
    190              // Put code here to handle WatchDog reset
    191            }
    192            else  // Unknown reason - not expected.
